{
    "workbench.colorTheme": "Nextlevel",
    "workbench.preferredDarkColorTheme": "Nextlevel",
    "workbench.preferredHighContrastColorTheme": "Nextlevel",
    "workbench.preferredHighContrastLightColorTheme": "Nextlevel",
    "workbench.preferredLightColorTheme": "Nextlevel",
    "workbench.iconTheme": "eq-material-theme-icons",
    "BioSyntax.nonBioTheme": "Nextlevel",
    "jupyter.themeMatplotlibPlots": true,
    "r.plot.defaults.colorTheme": "vscode",
    "materialTheme.accent": "Breaking Bad",
    "editor.tokenColorCustomizations": {
        "[*Light*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#000"
                    }
                }
            ]
        },
        "[*Dark*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#fff"
                    }
                }
            ]
        }
    },
    "cSpell.words": [
        "contigs"
    ]
}